<h1>Super Cool Session Practice</h1>

<h2>Log in</h2>

<form action="/authenticate" method="post">
  <div>
    <label for="the_username">Username:</label>
    <input type="text" id="the_username" name="username">
  </div>

  <div>
    <label for="the_password">Password:</label>
    <input type="password" id="the_password" name="userpassword">
  </div>

  <button type="submit">
    Log in
  </button>
</form>
